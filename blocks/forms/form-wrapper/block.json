{
	"apiVersion": 2,
	"name": "mailster/form-wrapper",
	"version": "0.1.0",
	"title": "Mailster Form",
	"icon": "email",
	"category": "widgets",
	"description": "Define the look of your form here.",
	"parent": ["core/post-content"],
	"supports": {
		"html": false,
		"inserter": false,
		"reusable": false,
		"multiple": false
	},
	"attributes": {
		"color": {
			"type": "string"
		},
		"backgroundColor": {
			"type": "string"
		},
		"fontSize": {
			"type": "integer"
		},
		"padding": {
			"type": "integer"
		},
		"borderRadius": {
			"type": "string"
		},
		"background": {
			"type": "object",
			"default": {
				"opacity": 100,
				"fixed": false,
				"repeat": false,
				"size": "cover",
				"image": "",
				"position": {
					"x": 0.5,
					"y": 0.5
				}
			}
		},
		"css": {
			"type": "object",
			"default": {
				"general": "",
				"tablet": "",
				"mobile": ""
			}
		},
		"messages": {
			"type": "object",
			"default": {
				"success": "#ffffff",
				"successBackground": "#6fbf4d",
				"error": "#ffffff",
				"errorBackground": "#bf4d4d"
			}
		}
	},
	"textdomain": "mailster",
	"editorScript": "file:./../../../build/forms/form-wrapper/index.js",
	"editorStyle": "file:./../../../build/forms/form-wrapper.css",
	"style": "file:./../../../build/style-form.css"
}
