{
	"apiVersion": 2,
	"name": "mailster/lists",
	"version": "0.1.0",
	"title": "Lists Field",
	"category": "mailster-form-fields",
	"keywords": ["container", "wrapper", "row", "section"],
	"parent": ["mailster/form-wrapper", "core/column", "core/group"],
	"description": "Simple Input Field for your Mailster Form",
	"supports": {
		"html": false,
		"reusable": false,
		"multiple": false
	},
	"attributes": {
		"dropdown": {
			"type": "boolean",
			"default": false
		},
		"lists": {
			"type": "array",
			"source": "query",
			"selector": "input",
			"default": [],
			"query": {
				"id": {
					"type": "string",
					"source": "attribute",
					"attribute": "value"
				},
				"name": {
					"type": "string",
					"source": "attribute",
					"attribute": "aria-label"
				},
				"checked": {
					"type": "string",
					"source": "attribute",
					"attribute": "checked"
				}
			}
		}
	},
	"textdomain": "mailster",
	"editorScript": "file:./../../../build/forms/lists/index.js"
}
