{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "mailster-workflow/action",
	"version": "0.1.0",
	"title": "Action",
	"category": "mailster-automation-actions",
	"description": "Execute an action",
	"parent": ["core/post-content", "mailster-workflow/condition"],
	"example": {
		"attributes": {
			"action": "add_list"
		}
	},
	"supports": {
		"html": false,
		"className": false,
		"customClassName": false
	},
	"attributes": {
		"id": {
			"type": "string"
		},
		"action": {
			"type": "string"
		},
		"lists": {
			"type": "array"
		},
		"tags": {
			"type": "array"
		},
		"field": {
			"type": "string"
		},
		"value": {
			"type": "string"
		},
		"webhook": {
			"type": "string"
		},
		"comment": {
			"type": "string"
		}
	},
	"textdomain": "mailster",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
