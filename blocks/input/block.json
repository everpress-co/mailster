{
	"apiVersion": 2,
	"name": "mailster/input",
	"version": "0.1.0",
	"title": "Input Field",
	"category": "mailster-form-fields",
	"keywords": ["container", "wrapper", "row", "section"],
	"icon": "button",
	"description": "Simple Input Field for your Mailster Form",
	"parent": ["mailster/form-wrapper"],
	"supports": {
		"html": false
	},
	"attributes": {
		"label": {
			"type": "string",
			"default": ""
		},
		"required": {
			"type": "boolean",
			"default": false
		},
		"optionalRequired": {
			"type": "boolean",
			"default": true
		},
		"inline": {
			"type": "boolean",
			"default": false
		},
		"type": {
			"type": "string",
			"default": "text"
		}
	},
	"textdomain": "mailster",
	"editorScript": "file:./../../build/input.js",
	"editorStyle": "file:./../../build/input.css",
	"style": "file:./../../build/style-input.css"
}
