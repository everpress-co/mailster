{
	"apiVersion": 2,
	"name": "mailster/button",
	"version": "0.1.0",
	"title": "Input Field",
	"category": "mailster-form-fields",
	"keywords": ["container", "wrapper", "row", "section"],
	"icon": "button",
	"description": "Simple Input Field for your Mailster Form",
	"parent": ["mailster/form-wrapper", "core/column"],
	"supports": {
		"html": false,
		"multiple": false
	},
	"icon": {
		"background": "#ff0",
		"src": "button"
	},
	"attributes": {
		"label": {
			"type": "string",
			"default": "Submit",
			"source": "attribute",
			"selector": "input",
			"attribute": "value"
		},
		"style": {
			"type": "object",
			"default": {
				"width": null
			}
		}
	},
	"textdomain": "mailster",
	"editorScript": "file:./../../build/button.js",
	"editorStyle": "file:./../../build/button.css",
	"style": "file:./../../build/style-button.css"
}
