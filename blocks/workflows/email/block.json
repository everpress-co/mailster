{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "mailster-workflow/email",
	"version": "0.1.0",
	"title": "Email",
	"category": "mailster-automation-fields",
	"description": "Send out an campaign or email.",
	"parent": ["core/post-content", "mailster-workflow/condition"],
	"example": {},
	"supports": {
		"html": false,
		"className": false,
		"customClassName": false,
		"reusable": false
	},
	"attributes": {
		"id": {
			"type": "string"
		},
		"name": {
			"type": "string"
		},
		"campaign": {
			"type": "integer"
		},
		"subject": {
			"type": "string"
		},
		"preheader": {
			"type": "string"
		},
		"comment": {
			"type": "string"
		}
	},
	"textdomain": "mailster",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
